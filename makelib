cc -fPIC -O3 -g -c libdyn.c -o libdyn.o
cc -fPIC -O3 -g -c libdyn_blocks.c -o libdyn_blocks.o
cc -fPIC -O2 -g -c block_lookup.c -o block_lookup.o
cc -fPIC -O2 -g -c irpar.c -o irpar.o
cc -fPIC -O2 -g -c log.c -o log.o
cc -fPIC -O2 -c libilc.c -o libilc.o
cc -fPIC -O2 -g -c libdyn_cpp.cpp -o libdyn_cpp.o

ld -shared libdyn.o libdyn_blocks.o libdyn_cpp.o block_lookup.o irpar.o log.o libilc.o -lm -lpthread -lrt -o libortd.so

sudo cp libortd.so /usr/local/lib
sudo ldconfig

#c++ -O2 -lgenric -lm dynlib_test4.cpp -o dynlib_test4
c++ -I.. -O2 -lortd -lm libdyn_generic_exec.cpp -o libdyn_generic_exec
sudo cp libdyn_generic_exec /usr/local/bin
